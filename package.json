{
  "name": "stormpath-widget",
  "version": "0.0.1",
  "description": "Stormpath JavaScript widget.",
  "author": "Stormpath, Inc.",
  "license": "Apache-2.0",
  "files": [
    "*.md",
    "dist"
  ],
  "main": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/stormpath/stormpath-widget.git"
  },
  "homepage": "https://stormpath.github.io/stormpath-widget/",
  "bugs": "https://github.com/stormpath/stormpath-widget/issues",
  "scripts": {
    "build": "NODE_ENV=production webpack src/index.js dist/stormpath.js && NODE_ENV=production webpack -p src/index.js dist/stormpath.min.js",
    "dev": "NODE_ENV=development webpack src/index.js dist/stormpath.dev.js --watch",
    "dev-browser": "open ./example/login/index.html && npm run dev",
    "eslint": "./node_modules/.bin/eslint -c .eslintrc src/",
    "test": "npm run eslint"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.9",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "html-loader": "^0.4.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "rivets": "^0.9.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
