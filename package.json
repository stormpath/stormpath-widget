{
  "name": "stormpath-widget",
  "version": "0.0.1",
  "description": "Stormpath JavaScript widget.",
  "author": "Stormpath, Inc.",
  "license": "Apache-2.0",
  "files": [
    "*.md",
    "dist"
  ],
  "main": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/stormpath/stormpath-widget.git"
  },
  "homepage": "https://stormpath.github.io/stormpath-widget/",
  "bugs": "https://github.com/stormpath/stormpath-widget/issues",
  "scripts": {
    "build": "NODE_ENV=production webpack src/index.js dist/stormpath.js --display-modules && NODE_ENV=production webpack -p src/index.js dist/stormpath.min.js --display-modules",
    "dev": "node dev-server.js",
    "karma": "./node_modules/karma/bin/karma start",
    "karma:watch": "npm run karma -- --no-single-run --auto-watch",
    "lint": "./node_modules/.bin/esw -c .eslintrc src test",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run lint && npm run karma"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.9",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "form-urlencoded": "^1.5.0",
    "html-loader": "^0.4.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "rivets": "^0.9.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "xhr": "^2.3.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "karma": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "webpack-dev-middleware": "^1.8.4"
  }
}
